    <link rel="stylesheet" href="~/vendor/libs/&#64;form-validation/form-validation.css" />

    <style>

        @@keyframes fadeInOut {
            0% {
                opacity: 0;
                transform: translateY(-10px);
            }

            50% {
                opacity: 1;
                transform: translateY(0);
            }

            100% {
                opacity: 0.5;
                transform: translateY(10px);
            }
        }

        .animated-text {
            font-size: 1.5rem;
            font-weight: bold;
            color: #8176f2;
            animation: fadeInOut 2s infinite ease-in-out;
        }
    </style>
}

